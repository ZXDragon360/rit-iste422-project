JC = javac;
JGLAGS = -g;
.SUFFIXES: .java .class
.java.class:
	$(JC) $(JFLAGS) $*.java

CLASSES = \
					src/java/CreateDDLMySQL.java\
					src/java/EdgeConnector.java\
					src/java/EdgeConvertCreateDDL.java\
					src/java/EdgeConvertFileParser.java\
					src/java/EdgeConvertGUI.java\
					src/java/EdgeField.java\
					src/java/EdgeTable.java\
					src/java/ExampleFileFilter.java\
					src/java/RunEdgeConvert.java

# default
default: classes

# target entries
classes: $(CLASSES:.java=.class)


# clean all class files
clean:
	$(RM) *.class
